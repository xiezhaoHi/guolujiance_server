#formdevicescontainer {
    background-color: rgb(0, 255, 0);
}

QHeaderView::section {
    background-color: rgb(241, 243, 248);
    color: black;
    padding-left: 8px;
    border: 0px solid #6c6c6c;
    font-size:11pt;
    height: 26px;
}

QTreeView {
    font-size:10pt;
    background-color: rgb(252, 252, 252);
}

QScrollBar:vertical {
    background-color: rgb(241, 243, 248);
    width: 9px;
    margin: 0px 0 0px 0;
}

QScrollBar:horizontal {
    background-color: rgb(241, 243, 248);
    height: 9px;
    margin: 0px 0 0px 0;
}

QScrollBar::handle:vertical {
    background: rgb(195, 195, 195);
    min-height: 20px;
    margin: 0 1px 0 2px;
    border-radius: 3px;
    border: none;
}

QScrollBar::handle:horizontal {
    background: rgb(195, 195, 195);
    min-width: 20px;
    margin: 1px 0 2px 0;
    border-radius: 3px;
    border: none;
}